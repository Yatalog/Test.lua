loadstring(game:HttpGet('https://raw.githubusercontent.com/Unknownproooolucky/Unknown-Hub-X-Universal-Games/main/Games/Evade'))()
local t = Instance.new("TextLabel", Instance.new("ScreenGui", game.Players.LocalPlayer.PlayerGui))
t.Text, t.Size, t.Position, t.TextColor3, t.Font, t.TextSize, t.BackgroundTransparency = "https://t.me/ptofic", UDim2.new(0,400,0,60), UDim2.new(0.5,-200,0.25,-30), Color3.fromRGB(255,87,51), Enum.Font.Arial, 16, 1
while wait(3) do t.TextColor3 = Color3.new(math.random(),math.random(),math.random()) end
